<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
   <title>Car Index</title>
  </head>
  <body>
   
    <h1>All Cars</h1>
    <nav>
      <ul>
        <li>
          <a href="/cars/new">Add New Car</a>
          
        </li>
      </ul>
    </nav>
   
    <nav>
      <ul>
         <% cars.forEach(car => { %>
            <li>
            <a href="/cars/<%=car._id %>">  
              <%= car.name %>
              </a>
            </li>
      
        <% }); %>
      </ul>
    </nav>
    <ul>
         
        
         </ul>
  </body>
